<mujoco>
  <compiler texturedir="texture/"/>
	<asset>
		<texture file="wood.png"/>
		<texture name="wood2d" type="2d" file="wood.png"/>
		<texture file="square.png"/>
		<texture name="foil" file="silverRaw.png"/>
    <texture file="woodb.png"/>
    <texture file="darkwood.png"/>

		<material name="MatFoil" texture="foil" specular="1" shininess=".3"  rgba=".9 .9 .9 1"/>
		<material name="MatMetal" specular="1" shininess=".3"  rgba=".9 .9 .9 1"/>
		<material name="MatYellow" specular="1" shininess=".3"  rgba=".8 .75 .05 1"/>
		<material name="MatGreen" specular="1" shininess=".3"  rgba=".2 .7 .2 1"/>
		<material name="MatDark" specular="1" shininess=".3"  rgba=".3 .3 .3 1"/>
		<material name="MatBrown" specular="1" shininess=".3"  rgba="0.6 0.5 0.38 1"/>
		<material name="MatPlane" specular=".3" shininess=".3" rgba=".3 .3 .2 1"/>
    <material name="MatWood" texture="wood" specular="0.4" shininess=".1" texrepeat="3 3"/>
    <material name="MatWoodDark" texture="darkwood" specular="0.4" shininess=".1" texrepeat="3 3"/>
    <material name="MatSquare" texture="square" specular="1" shininess=".6"  rgba=".8 .8 .8 1"/>
    <material name="MatWoodR" texture="wood" specular="1" shininess=".3"  rgba="1 0.5 0.5 1"/>
    <material name="MatWoodG" texture="wood" specular="1" shininess=".3"  rgba="0.2 1 0.2 1"/>
    <material name="MatWoodB" texture="woodb" specular="1" shininess=".3"/>
	</asset>
</mujoco>